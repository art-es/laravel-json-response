{
    "name": "art-es/laravel-json-response",
    "license": "MIT",
    "description": "Package extending laravel json response.",
    "authors": [
        {
            "name": "art-es",
            "email": "sh.artur.ilf@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^6.0",
        "ext-json": "*"
    }
    ,
    "require-dev": {
        "phpunit/phpunit": "7.*"
    },
    "autoload": {
        "psr-4": {
            "Artes\\JsonResponse\\": "src/JsonResponse/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Artes\\JsonResponse\\JsonResponseServiceProvider"
            ],
            "aliases": {
                "JsonResponse": "Artes\\JsonResponse\\Facades\\JsonResponse"
            }
        }
    }
}
